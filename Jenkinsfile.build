pipeline {
    agent any
    parameters {
        booleanParam(defaultValue: true, description: "Do deploy after build", name: "IS_SKIP_TEST")
        string(name: "BRANCH_NAME", defaultValue: "master", description: "")
    }
    stages {
        stage('Test') {
            steps {
                echo params.BRANCH_NAME
                echo "${params.IS_SKIP_TEST}"
            }
        }
    }
}